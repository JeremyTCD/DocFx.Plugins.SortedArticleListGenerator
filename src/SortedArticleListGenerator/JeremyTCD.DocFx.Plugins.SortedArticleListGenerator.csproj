<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net462</TargetFramework>
        <AssemblyName>JeremyTCD.DocFx.Plugins.SortedArticleListGenerator</AssemblyName>
        <PackageId>JeremyTCD.DocFx.Plugins.SortedArticleListGenerator</PackageId>
        <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
        <GenerateAssemblyTitleAttribute>false</GenerateAssemblyTitleAttribute>
        <GenerateAssemblyCopyrightAttribute>false</GenerateAssemblyCopyrightAttribute>
        <Authors>JeremyTCD</Authors>
        <Copyright>Copyright © JeremyTCD 2017-2018</Copyright>
        <Description>A DocFx post-processor for article list generation.</Description>
        <PackageLicenseUrl>https://github.com/JeremyTCD/DocFx.Plugins.SortedArticleListGenerator/blob/master/License.txt</PackageLicenseUrl>
        <PackageProjectUrl>https://github.com/JeremyTCD/DocFx.Plugins.SortedArticleListGenerator</PackageProjectUrl>
        <RepositoryUrl>https://github.com/JeremyTCD/DocFx.Plugins.SortedArticleListGenerator</RepositoryUrl>
        <RepositoryType>GitHub</RepositoryType>
        <NeutralLanguage>English</NeutralLanguage>
        <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="JeremyTCD.DocFx.Plugins.Utils" Version="0.5.0" />
        <PackageReference Include="Microsoft.Composition" Version="1.0.31" />
        <PackageReference Include="Microsoft.DocAsCode.Build.ConceptualDocuments" Version="2.39.3" />
        <PackageReference Include="Microsoft.DocAsCode.Common" Version="2.39.3" />
        <PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
    </ItemGroup>

</Project>
